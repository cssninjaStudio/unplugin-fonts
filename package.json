{
  "name": "unplugin-fonts",
  "version": "1.3.1",
  "description": "Universal Webfont loader",
  "author": {
    "name": "cssninjaStudio <hello@cssninja.io> (https://cssninja.io)"
  },
  "license": "MIT",
  "repository": "cssninjaStudio/unplugin-fonts",
  "keywords": [
    "fonts",
    "google-fonts",
    "typekit",
    "fontsource",
    "webfontloader",
    "svelte",
    "nuxt",
    "vite",
    "vitepress",
    "astro-integration"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./astro": {
      "types": "./dist/astro.d.ts",
      "import": "./dist/astro.mjs",
      "require": "./dist/astro.js"
    },
    "./astro/component.astro": {
      "types": "./dist/astro/component.d.ts",
      "import": "./dist/astro/component.astro"
    },
    "./esbuild": {
      "types": "./dist/esbuild.d.ts",
      "import": "./dist/esbuild.mjs",
      "require": "./dist/esbuild.js"
    },
    "./nuxt": {
      "types": "./dist/nuxt.d.ts",
      "import": "./dist/nuxt.mjs",
      "require": "./dist/nuxt.js"
    },
    "./rollup": {
      "types": "./dist/rollup.d.ts",
      "import": "./dist/rollup.mjs",
      "require": "./dist/rollup.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.mjs",
      "require": "./dist/types.js"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.mjs",
      "require": "./dist/vite.js"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "import": "./dist/webpack.mjs",
      "require": "./dist/webpack.js"
    },
    "./client": {
      "types": "./client.d.ts"
    },
    "./*": "./*"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "client.d.ts",
    "dist"
  ],
  "scripts": {
    "prepack": "npm run build",
    "build": "tsup",
    "dev": "tsup --watch src",
    "build:fix": "esno scripts/postbuild.ts",
    "example:dev": "npm -C examples/vite run dev",
    "example:build": "npm -C examples/vite run build",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "vitest",
    "release": "npm run lint && npx standard-version && git push --follow-tags origin main && npm publish"
  },
  "peerDependencies": {
    "@nuxt/kit": "^3.0.0",
    "vite": "^2.0.0 || ^3.0.0 || ^4.0.0 || ^5.0.0 || ^6.0.0"
  },
  "peerDependenciesMeta": {
    "@nuxt/kit": {
      "optional": true
    }
  },
  "dependencies": {
    "fast-glob": "^3.3.2",
    "unplugin": "2.0.0-beta.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.11.2",
    "@nuxt/kit": "^3.14.1592",
    "@nuxt/schema": "^3.14.1592",
    "@types/node": "^22.10.1",
    "astro": "^4.16.16",
    "chalk": "^5.3.0",
    "defu": "^6.1.4",
    "eslint": "^9.15.0",
    "esno": "^4.8.0",
    "magic-string": "^0.30.14",
    "pathe": "^1.1.2",
    "rollup": "^4.27.4",
    "standard-version": "^9.5.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vite": "^6.0.1",
    "vitest": "^2.1.6",
    "webpack": "^5.96.1"
  }
}
